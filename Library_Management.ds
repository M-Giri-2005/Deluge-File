			Add_Qty as "Add Qty"
			{
				type =  form
				form = Books_Return
				record event = on add or edit

				on success
				{
					actions 
					{
						custom deluge script
						(
											fetch_Member_Details = Membership_Details[Member_ID == input.Member_ID];
										Giri = Books_Issue_Existing[Books_ID == input.Books_Return_Details.Books_ID];
										Dilli = Books_Issue[ID == input.Books_Issue_ID];
										for each  rec in input.Books_Return_Details
										{
											if(rec.Books_Status == "Books Return")
											{
												Fetch_Books_Detils = Books_Inventory[ID == rec.Books_ID];
												Fetch_Books_Detils.Books_Qty=Fetch_Books_Detils.Books_Qty + 1;
												fetch_Member_Details.Prv_Take_Book=fetch_Member_Details.Prv_Take_Book - 1;
												fetch_Member_Details.Total_Book_Count=fetch_Member_Details.Total_Book_Count + 1;
												Giri.Books_Status="Books Return";
												Dilli.Book_Count_Issue=Dilli.Book_Count_Issue - 1;
												sendmail
												[
													from :zoho.loginuserid
													to :input.Member_Email
													subject :"Your Book Return ID"
													message :input.Books_Return_ID
												]
											}
											if(input.Books_Return_Details.Books_Status == "Books extension")
											{
												Giri.Book_Issue_Date=zoho.currentdate;
												Giri.Books_Return_Date=zoho.currentdate.addDay(fetch_Member_Details.Books_Take_Available_Days);
												sendmail
												[
													from :zoho.loginuserid
													to :input.Member_Email
													subject :"Your Book Isue ID" + " && " + "Books Return Date"
													message :input.Books_Issue_ID + " && " + rec.Books_Return_Date
												]
											}
										}
										input.Books_Return_ID = input.Member_Name.subString(0,3) + "_" + input.Books_Return_Details.Books_Return_Date + "_" + input.Auto_Number;
						)
					}
				}

			}
