				record event = on add or edit

				on user input of Total_Issue_Books
				{
					actions 
					{
						custom deluge script
						(
											if(input.Total_Book_Count == null)
										{
											if(input.Total_Issue_Books != null && input.Total_Issue_Books < 0)
											{
												alert "Not Valid Reson For You Not Enter The Member ID";
												input.Total_Issue_Books = null;
											}
										}
										if(input.Total_Book_Count < input.Total_Issue_Books)
										{
											alert "Not Valid Reson For You AllReady Reach You Books Limit";
											input.Total_Issue_Books = 0;
										}
										if(input.Total_Issue_Books == null || input.Total_Issue_Books <= 0)
										{
											input.Books_Details.clear();
										}
										else if(input.Total_Book_Count >= input.Total_Issue_Books && input.Total_Issue_Books != 0 && input.Sub_Count == 1)
										{
											input.Books_Details.clear();
											Leftpad = "".leftpad(input.Total_Issue_Books - 1).toList("");
											for each  rec in Leftpad
											{
												var = Books_Issue.Books_Details();
												var.S_no=0;
												ver = Collection();
												ver.insert(var);
												input.Books_Details.insert(ver);
											}
										}
										if(input.Total_Issue_Books != null)
										{
											input.Book_Count_Issue = input.Total_Issue_Books;
										}
										if(input.Total_Issue_Books != null && input.Total_Issue_Books > 0)
										{
											disable Total_Issue_Books;
											input.Sub_Count = 0;
										}
										if(input.Total_Issue_Books < 0)
										{
											alert "Not Valid Reson For You Enter The Negative Value";
											input.Total_Issue_Books = null;
										}
						)
					}
